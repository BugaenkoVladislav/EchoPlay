@model dynamic

@{
    ViewBag.Title = "SignIn";
    Layout = "_Layout";
}
<h2>SignIn</h2>
<form asp-controller="Authentication" asp-action="Identify" method="post">
    <h4>Login</h4>
    <input type="email" name="Email"/>
    <h4>Password</h4>
    <input type="password" name="Password"/>
    <input type="submit" value="Enter"/>
</form>
<form asp-controller="Authentication" asp-action="LoginGoogle" method="post">
    <input type="submit" value="Google" />
</form>
<a asp-controller="Authentication" asp-action="SignUp">SignUp</a>