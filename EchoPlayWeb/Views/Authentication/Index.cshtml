@model dynamic

@{
    ViewBag.Title = "SignIn";
    Layout = "_Layout";
}
<h2>SignIn</h2>
<form method="post">
    <h4>Login</h4>
    <input type="email" name="Login"/>
    <h4>Password</h4>
    <input type="password" name="Password"/>
    <input type="submit" value="Enter"/>
</form>
<a href="http://localhost:8080">SignUp</a>