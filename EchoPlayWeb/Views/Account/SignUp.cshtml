@model dynamic

@{
    ViewBag.Title = "SignUp";
    Layout = "_Layout";
}
<h2>SignIn</h2>
<form method="post">
    <h4>Username</h4>
    <input type="text" name="Username"/>
    <h4>Login</h4>
    <input type="email" name="Email"/>
    <h4>Password</h4>
    <input type="password" name="Password"/>
    <input type="submit" value="Enter"/>
</form>